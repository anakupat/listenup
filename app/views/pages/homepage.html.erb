<center>
<div class="jumbotron" style="max-width: 400px">
<% if current_user %>
  <img src="<%= current_user.url %>" class="img-circle"><br>

  <h4>Welcome <%= current_user.name %> !</h4>
  <a href= "<%= signout_path %>" class="btn btn-success" role="button">Sign Out</a>

<% else %>
  <a href= "/auth/twitter" class="btn btn-success" role="button">Sign In with Twitter</a>
<% end %>
</div>

</center>




